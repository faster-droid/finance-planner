{
    "dashboard": {
      "id": null,
      "uid": null,
      "title": "Finance App Dashboard",
      "tags": ["finance"],
      "timezone": "browser",
      "schemaVersion": 16,
      "version": 0,
      "refresh": "5s",
      "panels": [
        {
          "type": "graph",
          "title": "Total Requests",
          "targets": [
            {
              "expr": "sum(rate(http_requests_total[5m])) by (path)",
              "legendFormat": "{{path}}",
              "refId": "A"
            }
          ],
          "xaxis": {
            "show": true
          },
          "yaxes": [
            {
              "format": "short",
              "label": "Requests per second",
              "show": true
            }
          ]
        },
        {
          "type": "graph",
          "title": "90th Percentile Response Time",
          "targets": [
            {
              "expr": "histogram_quantile(0.9, sum(rate(http_request_duration_seconds_bucket[5m])) by (le))",
              "legendFormat": "90th Percentile",
              "refId": "B"
            }
          ],
          "xaxis": {
            "show": true
          },
          "yaxes": [
            {
              "format": "s",
              "label": "Seconds",
              "show": true
            }
          ]
        }
      ]
    },
    "overwrite": true
  }